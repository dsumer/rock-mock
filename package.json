{
  "name": "rock-mock",
  "version": "0.0.1",
  "description": "it's the solid rock for providing a mock.",
  "main": "index.js",
  "repository": "git@github.com:dsumer/rock-mock.git",
  "author": "Dominik Sumer <domi.sumer@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build-client && npm run start-server",
    "start-server": "nodemon --inspect index.js",
    "start-client": "parcel watch ./src/client/app/main.js -d ./src/client/build --public-url=./",
    "build-client": "parcel build ./src/client/app/main.js -d ./src/client/build --public-url=./"
  },
  "dependencies": {
    "koa": "^2.5.3",
    "koa-mount": "^3.0.0",
    "koa-static": "^5.0.0",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.2.0",
    "nodemon": "^1.18.4",
    "parcel": "^1.9.7",
    "vue-template-compiler": "^2.5.17"
  }
}
